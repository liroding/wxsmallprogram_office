/* pages/wxml/index.wxss */
@import '/lib/weui.wxss';

.container{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: relative;
}
image{
  position: absolute;
  width: 100%;
  height: 100%;
}

.btn1 {
  width: 80%;
  margin-top: 900rpx;
  background-color: burlywood;
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}


.btn2 {
  width: 80%;
  margin-bottom: 200rpx;
  background-color: rgb(243, 161, 53);
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.btn1::after {
  border-radius: 98rpx;
  border: 0; 
}





